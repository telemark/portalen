{
  "name": "portalen",
  "version": "1.0.0",
  "description": "Portal frontpage for Telemark fylkeskommune",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "contributors": [
    {
      "name": "Jonas Enge",
      "email": "jonas.enge@gmail.com",
      "url": "https://github.com/maccyber"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10.14.1"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "node server.js",
    "build": "next build",
    "start": "node server.js",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "now && now alias"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "Tfk",
    "Portalen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/portalen.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/portalen/issues"
  },
  "homepage": "https://github.com/telemark/portalen#readme",
  "devDependencies": {
    "ava": "1.1.0",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.2",
    "micro-dev": "3.0.0",
    "nyc": "13.1.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "dotenv": "6.2.0",
    "jsonwebtoken": "8.4.0",
    "micro": "9.3.3",
    "micro-cookie-session": "2.0.0-beta.1",
    "moment": "2.23.0",
    "mongojs": "2.6.0",
    "next": "7.0.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-markdown": "4.0.6",
    "react-onclickoutside": "6.7.1",
    "tfk-unique-array": "1.0.11",
    "urlencoded-body-parser": "2.0.1",
    "uuid": "3.3.2",
    "wildcard": "1.1.2",
    "winston": "2.4.4",
    "winston-papertrail": "1.0.5"
  },
  "now": {
    "name": "portalen",
    "alias": "www.portalen.win",
    "type": "npm",
    "engines": {
      "node": "8.10.0"
    },
    "dotenv": "production.env"
  }
}
